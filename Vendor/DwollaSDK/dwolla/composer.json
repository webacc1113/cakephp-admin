{
    "name": "Dwolla/dwolla-php",
    "description": "An official Guzzle based wrapper for the Dwolla API.",
    "keywords": ["dwolla", "money", "payments", "api", "orange"],
    "homepage": "http://developers.dwolla.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Dwolla",
            "email": "api@dwolla.com",
            "homepage": "https://developers.dwolla.com/"
        },
        {
            "name": "David Stancu",
            "email": "dstancu@nyu.edu",
            "homepage": "http://davidstancu.me"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-json": "*",
        "ext-curl": "*" ,
        "guzzlehttp/guzzle": ">=4.2.0"
    },
    "require-dev": {
        "psr/log": ">=1.0",
        "phpunit/phpunit": ">=4.0.0"
    },
    "autoload": {
        "psr-0": {
            "Dwolla\\" : "lib/"
        },
        "files": [
            "lib/client.php",
            "lib/account.php",
            "lib/checkouts.php",
            "lib/contacts.php",
            "lib/fundingSources.php",
            "lib/masspay.php",
            "lib/oauth.php",
            "lib/requests.php",
            "lib/transactions.php"
        ]
    }
}
